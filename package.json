{
  "name": "cloud-control-tower",
  "description": "Decisions and handbook for implementing AWS Control Tower",
  "author": {
    "name": "James Ramirez"
  },
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "only-allow pnpm",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{json,ts,tsx,md,yml,astro}\""
  },
  "devDependencies": {
    "only-allow": "1.2.1",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "turbo": "2.5.4",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.5.0",
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ],
    "overrides": {
      "sharp@<0.32.6": ">=0.32.6"
    }
  }
}
